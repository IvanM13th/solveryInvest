pipeline {
  agent any
  stages {
    stage("build") {
      steps {
        echo 'building the application...'
      }
    }
    stage("test") {
      withMaven(
        maven: Maven) {
        sh "mvn clean test"
        }
    }
    stage("deploy") {
      steps {
        echo 'deploying the application...'
      }
    }
  }
}
